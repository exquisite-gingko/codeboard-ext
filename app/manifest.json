{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/Very-Basic-Code-icon.png",
    "128": "images/Very-Basic-Code-icon.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "background",
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "cookies",
    "*://*.google.com",
    "debugger",
    "history",
    "idle",
    "management",
    "notifications",
    "pageCapture",
    "topSites",
    "storage",
    "webNavigation",
    "webRequest"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": false
  },
  "omnibox": {
    "keyword": "OMNIBOX-KEYWORD"
  },
  "browser_action": {
    "default_icon": "images/115090-magic-marker-icon-business-pencil7-sc49.png",
    "default_title": "CodeBoard",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "images/115090-magic-marker-icon-business-pencil7-sc49.png"
  ]
}
